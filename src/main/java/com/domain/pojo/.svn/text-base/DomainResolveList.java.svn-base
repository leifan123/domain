package com.domain.pojo;

import java.io.Serializable;

public class DomainResolveList  implements Serializable{

	private static final long serialVersionUID = 1534211779261615623L;
	private Integer id;

	public DomainResolveList setId(Integer id){
		 this.id = id;
		 return this;
	}

	public Integer getId(){
		 return id;
	}
	private String userId;

	public DomainResolveList setUserId(String userId){
		 this.userId = userId;
		 return this;
	}

	public String getUserId(){
		 return userId;
	}
	private String companyid;

	public DomainResolveList setCompanyid(String companyid){
		 this.companyid = companyid;
		 return this;
	}

	public String getCompanyid(){
		 return companyid;
	}
	private String domainName;

	public DomainResolveList setDomainName(String domainName){
		 this.domainName = domainName;
		 return this;
	}

	public String getDomainName(){
		 return domainName;
	}
	private String recItem;

	public DomainResolveList setRecItem(String recItem){
		 this.recItem = recItem;
		 return this;
	}

	public String getRecItem(){
		 return recItem;
	}
	private String recType;

	public DomainResolveList setRecType(String recType){
		 this.recType = recType;
		 return this;
	}

	public String getRecType(){
		 return recType;
	}
	private Integer recLevel;

	public DomainResolveList setRecLevel(Integer recLevel){
		 this.recLevel = recLevel;
		 return this;
	}

	public Integer getRecLevel(){
		 return recLevel;
	}
	private String recValue;

	public DomainResolveList setRecValue(String recValue){
		 this.recValue = recValue;
		 return this;
	}

	public String getRecValue(){
		 return recValue;
	}
	private Integer recTtl;

	public DomainResolveList setRecTtl(Integer recTtl){
		 this.recTtl = recTtl;
		 return this;
	}

	public Integer getRecTtl(){
		 return recTtl;
	}
	private String recId;

	public DomainResolveList setRecId(String recId){
		 this.recId = recId;
		 return this;
	}

	public String getRecId(){
		 return recId;
	}
	private Integer resolveCode;

	public DomainResolveList setResolveCode(Integer resolveCode){
		 this.resolveCode = resolveCode;
		 return this;
	}

	public Integer getResolveCode(){
		 return resolveCode;
	}
	private String remark;

	public DomainResolveList setRemark(String remark){
		 this.remark = remark;
		 return this;
	}

	public String getRemark(){
		 return remark;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override 
	//显示错误信息的toString
	public String toString() {
		String Info = "";
		if(domainName!=null){
			Info = Info+"域名=" + domainName;
		}
		if(recItem!=null){
			Info = Info+",主机头=" + recItem;
		}
		if(recType!=null){
			Info = Info+",记录类型=" + recType;
		}
		if(recLevel!=null){
			Info = Info+",优先级=" + recLevel;
		}
		if(recValue!=null){
			Info = Info+", 记录值=" + recValue;
		}
		if(recTtl!=null){
			Info = Info+", TTL值=" + recTtl;
		}

		return Info+"";
	}
	
	
	
}